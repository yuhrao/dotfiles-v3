  },
  {
    "context": "ProjectPanel",
    "bindings": {
      "\\": "workspace::ToggleLeftDock",
      "Y": "project_panel::Copy",
      "y": "project_panel::Copy"
    }
  },
  {
    "context": "Terminal",
    "bindings": {
      "escape escape": "terminal::Clear",
      "ctrl-h": "pane::ActivatePrevItem",
      "ctrl-l": "pane::ActivateNextItem",
      "ctrl-j": "pane::SplitDown",
      "ctrl-k": "pane::SplitUp"
    }
  }
]
